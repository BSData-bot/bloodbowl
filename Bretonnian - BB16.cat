                                <characteristic name="AV" typeId="415623232344415441232323">4</characteristic>
                                <characteristic name="Skills" typeId="536b696c6c7323232344415441232323">Dodge, Leap, No Hands, Stunty, Titchy, Very Long Legs</characteristic>
